<?xml version="1.0" encoding="utf-8"?>
<resources>


    <attr name="aspect" />

    <declare-styleable name="GPUImageView">
        <attr name="gpuimage_surface_type" format="enum">
            <enum name="surface_view" value="0" />
            <enum name="texture_view" value="1" />
        </attr>
        <attr name="gpuimage_show_loading" format="boolean" />
    </declare-styleable>

    <declare-styleable name="RangeSeekBar">
        <attr name="max" format="float" />
        <attr name="min" format="float" />
        <attr name="reserve" format="float" />
        <attr name="cells" format="integer" />
        <attr name="progressHintMode" format="enum">
            <enum name="defaultMode" value="0" />
            <enum name="alwaysHide" value="1" />
            <enum name="alwaysShow" value="2" />
        </attr>
        <attr name="lineColorSelected" format="color" />
        <attr name="lineColorEdge" format="color" />
        <attr name="thumbPrimaryColor" format="color" />
        <attr name="thumbSecondaryColor" format="color" />
        <attr name="markTextArray" format="reference" />
        <attr name="thumbResId" format="reference" />
        <!-- must use 9 path ！！！-->
        <attr name="progressHintResId" format="reference" />
        <attr name="textPadding" format="dimension" />
        <attr name="textSize" format="dimension" />
        <attr name="hintBGHeight" format="dimension" />
        <attr name="hintBGWith" format="dimension" />
        <attr name="hintBGPadding" format="dimension" />
        <attr name="seekBarHeight" format="dimension" />
        <attr name="thumbSize" format="dimension" />
        <attr name="cellMode" format="enum">
            <enum name="number" value="0" />
            <enum name="other" value="1" />
        </attr>
        <attr name="seekBarMode" format="enum">
            <enum name="single" value="1" />
            <enum name="range" value="2" />
        </attr>

    </declare-styleable>

    <declare-styleable name="RippleView">
        <attr name="rv_alpha" format="integer" />
        <attr name="rv_framerate" format="integer" />
        <attr name="rv_rippleDuration" format="integer" />
        <attr name="rv_zoomDuration" format="integer" />
        <attr name="rv_color" format="color" />
        <attr name="rv_centered" format="boolean" />
        <attr name="rv_type" format="enum">
            <enum name="simpleRipple" value="0" />
            <enum name="doubleRipple" value="1" />
            <enum name="rectangle" value="2" />
        </attr>
        <attr name="rv_ripplePadding" format="dimension" />
        <attr name="rv_zoom" format="boolean" />
        <attr name="rv_zoomScale" format="float" />

    </declare-styleable>

    <declare-styleable name="ForegroundImageView">
        <attr name="android:foreground" />
    </declare-styleable>

    <declare-styleable name="CircleButton">
        <attr name="cb_color" format="color" />
        <attr name="cb_pressedRingWidth" format="dimension" />
    </declare-styleable>


    <declare-styleable name="Themp">
        <attr name="colorPrimary" format="color|reference" />
    </declare-styleable>


    <declare-styleable name="GestureView">
        <attr name="gest_movementAreaWidth" format="reference|dimension" />
        <attr name="gest_movementAreaHeight" format="reference|dimension" />
        <attr name="gest_minZoom" format="reference|float" />
        <attr name="gest_maxZoom" format="reference|float" />
        <attr name="gest_doubleTapZoom" format="reference|float" />
        <attr name="gest_overzoomFactor" format="reference|float" />
        <attr name="gest_overscrollX" format="reference|dimension" />
        <attr name="gest_overscrollY" format="reference|dimension" />
        <attr name="gest_fillViewport" format="reference|boolean" />
        <attr name="gest_gravity">
            <!-- Values are copied from system "gravity" attribute -->
            <flag name="top" value="0x30" />
            <flag name="bottom" value="0x50" />
            <flag name="left" value="0x03" />
            <flag name="right" value="0x05" />
            <flag name="center_vertical" value="0x10" />
            <flag name="center_horizontal" value="0x01" />
            <flag name="center" value="0x11" />
            <flag name="start" value="0x00800003" />
            <flag name="end" value="0x00800005" />
        </attr>
        <attr name="gest_fitMethod">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
            <enum name="inside" value="2" />
            <enum name="outside" value="3" />
            <enum name="none" value="4" />
        </attr>
        <attr name="gest_boundsType">
            <enum name="normal" value="0" />
            <enum name="inside" value="1" />
            <enum name="outside" value="2" />
            <enum name="pivot" value="3" />
            <enum name="none" value="4" />
        </attr>
        <attr name="gest_panEnabled" format="reference|boolean" />
        <attr name="gest_flingEnabled" format="reference|boolean" />
        <attr name="gest_zoomEnabled" format="reference|boolean" />
        <attr name="gest_rotationEnabled" format="reference|boolean" />
        <attr name="gest_restrictRotation" format="reference|boolean" />
        <attr name="gest_doubleTapEnabled" format="reference|boolean" />
        <attr name="gest_exitEnabled" format="reference|boolean" />
        <attr name="gest_animationDuration" format="reference|integer" />
        <attr name="gest_disableGestures" format="reference|boolean" />
        <attr name="gest_disableBounds" format="reference|boolean" />
    </declare-styleable>

    <!-- Added copy with GestureImageView name to enable autocomplete in AS -->
    <declare-styleable name="GestureImageView">
        <attr name="gest_movementAreaWidth" />
        <attr name="gest_movementAreaHeight" />
        <attr name="gest_minZoom" />
        <attr name="gest_maxZoom" />
        <attr name="gest_doubleTapZoom" />
        <attr name="gest_overzoomFactor" />
        <attr name="gest_overscrollX" />
        <attr name="gest_overscrollY" />
        <attr name="gest_fillViewport" />
        <attr name="gest_gravity" />
        <attr name="gest_fitMethod" />
        <attr name="gest_boundsType" />
        <attr name="gest_panEnabled" />
        <attr name="gest_flingEnabled" />
        <attr name="gest_zoomEnabled" />
        <attr name="gest_rotationEnabled" />
        <attr name="gest_restrictRotation" />
        <attr name="gest_doubleTapEnabled" />
        <attr name="gest_exitEnabled" />
        <attr name="gest_animationDuration" />
        <attr name="gest_disableGestures" />
        <attr name="gest_disableBounds" />
    </declare-styleable>

    <!-- Added copy with GestureFrameLayout name to enable autocomplete in AS -->
    <declare-styleable name="GestureFrameLayout">
        <attr name="gest_movementAreaWidth" />
        <attr name="gest_movementAreaHeight" />
        <attr name="gest_minZoom" />
        <attr name="gest_maxZoom" />
        <attr name="gest_doubleTapZoom" />
        <attr name="gest_overzoomFactor" />
        <attr name="gest_overscrollX" />
        <attr name="gest_overscrollY" />
        <attr name="gest_fillViewport" />
        <attr name="gest_gravity" />
        <attr name="gest_fitMethod" />
        <attr name="gest_boundsType" />
        <attr name="gest_panEnabled" />
        <attr name="gest_flingEnabled" />
        <attr name="gest_zoomEnabled" />
        <attr name="gest_rotationEnabled" />
        <attr name="gest_restrictRotation" />
        <attr name="gest_doubleTapEnabled" />
        <attr name="gest_exitEnabled" />
        <attr name="gest_animationDuration" />
        <attr name="gest_disableGestures" />
        <attr name="gest_disableBounds" />
    </declare-styleable>


    <declare-styleable name="CropAreaView">
        <attr name="gest_backgroundColor" format="reference|color" />
        <attr name="gest_borderColor" format="reference|color" />
        <attr name="gest_borderWidth" format="reference|dimension" />
        <attr name="gest_rulesHorizontal" format="reference|integer" />
        <attr name="gest_rulesVertical" format="reference|integer" />
        <attr name="gest_rulesWidth" format="reference|dimension" />
        <attr name="gest_rounded" format="reference|boolean" />
        <attr name="gest_aspect" format="reference|float" />
    </declare-styleable>


    <declare-styleable name="BlureView">
        <attr name="downSampling" format="integer" />
        <attr name="blurRadius" format="float" />
    </declare-styleable>

    <declare-styleable name="PxBlurringView">
        <attr name="blurRadius2" format="integer" />
        <attr name="downsampleFactor" format="integer" />
        <attr name="overlayColor" format="color" />
    </declare-styleable>


</resources>